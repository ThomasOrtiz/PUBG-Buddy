{
    "name": "pubg-buddy",
    "version": "1.0.0",
    "description": "A bot that reports PUBG stats",
    "main": "src/index.ts",
    "author": "Thomas Ortiz",
    "scripts": {
        "build:dev": "webpack --config webpack.config.dev.js",
        "build:prod": "webpack --config webpack.config.prod.js",
        "migrate": "node ./dist/migrate.bundle.js",
        "start": "node --optimize_for_size --max_old_space_size=460 dist/index.bundle.js",
        "sql": "heroku pg:psql -a pubg-stat-discord-bot",
        "sql-test": "heroku pg:psql -a pubg-stat-discord-bot-dev"
    },
    "dependencies": {
        "axios": "^0.21.1",
        "dblapi.js": "^2.3.0",
        "discord.js": "^11.5.1",
        "dotenv": "^8.1.0",
        "jimp": "^0.6.4",
        "mixpanel": "^0.10.2",
        "node-cache": "^4.2.1",
        "pg": "^7.12.1",
        "winston": "^3.2.1"
    },
    "devDependencies": {
        "@types/mixpanel": "^2.14.1",
        "@types/node": "^12.7.2",
        "@types/pg": "^7.11.0",
        "clean-webpack-plugin": "^1.0.0",
        "eslint": "^6.2.2",
        "progress-bar-webpack-plugin": "^1.12.1",
        "speed-measure-webpack-plugin": "^1.3.1",
        "ts-loader": "^6.0.4",
        "typescript": "^3.5.3",
        "webpack": "^4.39.2",
        "webpack-bundle-analyzer": "^3.4.1",
        "webpack-cli": "^3.3.7",
        "webpack-merge": "^4.2.1",
        "webpack-node-externals": "^1.7.2"
    }
}
