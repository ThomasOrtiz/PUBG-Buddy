{
  "env": {
    "es6": true,
    "node": true,
    "jquery": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2017
  },
  "rules": {
    "no-console": "off",
    "indent": ["error", 4, {
      "SwitchCase": 1
    }],
    "linebreak-style": ["error", "windows"],
    "quotes": ["warn", "single"],
    "semi": ["warn", "always"]
  }
}